<div class="container">
  <div class="row">
    <div class="col-sm-12">
      <h1>Productos</h1>

      <table class="table">
        <tr>
          <th>Nombre</th>
        </tr>
        <% @products.each do |product| %>
          <tr>
            <td><%= product.name %></td>
          </tr>
        <% end %>
      </table>
    </div>
  </div>
</div>